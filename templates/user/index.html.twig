{% extends 'base.html.twig' %}
{% block title %}List users{% endblock %}

{% block body %}
    <h1>List Users</h1>

    <ul>
        {% for user in users %}
            <li>
                <a href="{{ path('users.show', {user: user.id}) }}">{{ user.email }}</a>
            </li>
        {% endfor %}
    </ul>

    <a href="{{ path('users.create') }}">Create</a>
{% endblock %}